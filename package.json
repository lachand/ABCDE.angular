{
  "name": "Toccata",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/set-env.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=test && ng build ",
    "deploy": "npm run deploy-demo && npm run deploy-test && npm run deploy-renon",
    "deploy-demo": "npm run build-demo && npm run copy-firebase && npm run deploy-firebase-demo",
    "deploy-test": "npm run build-test && npm run copy-firebase && npm run deploy-fireabse-test",
    "deploy-renon": "npm run build-renon && npm run copy-firebase && npm run deploy-firebase-renon",
    "build-demo": "ng build --configuration=demo --build-optimizer",
    "build-test": "ng build --configuration=test --build-optimizer ",
    "build-renon": "ng build --configuration=production --build-optimizer",
    "copy-firebase": "node_modules/.bin/ngsw-config dist ./src/ngsw-config.json && cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/ngsw-worker.js",
    "deploy-firebase-demo": "firebase use fir-toccata && firebase deploy --only hosting:fir-toccata",
    "deploy-fireabse-test": "firebase use test-toccata && firebase deploy --only hosting:test-toccata",
    "deploy-firebase-renon": "firebase use toccata-958f9 && firebase deploy --only hosting:toccata-958f9",
    "test": "npm run config -- --environment=prod && ng test --prod --watch=false --progress=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.4.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.3",
    "@nicky-lenaers/ngx-scroll-to": "^1.1.1",
    "angular-material-fileupload": "^1.0.1",
    "angular-tslint-rules": "^1.5.0",
    "angular4-files-upload": "^1.0.2",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.16.4",
    "h5p-nodejs-library": "0.0.5",
    "hammerjs": "^2.0.8",
    "jqwidgets-scripts": "^6.0.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "navigator": "latest",
    "ng-animate": "^0.3.4",
    "ng-drag-drop": "^5.0.0",
    "ng2-ckeditor": "^1.2.1",
    "ng2-dragula": "^2.1.0",
    "ngx-calculator": "^1.0.8",
    "ngx-drag-drop": "^1.1.0",
    "ngx-file-drop": "^5.0.0",
    "node-sass": "^4.9.3",
    "pouchdb": "~6.4.3",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-find": "^7.0.0",
    "pouchdb-upsert": "^2.2.0",
    "require": "^2.4.20",
    "rxjs": "^6.0.0",
    "timer-stopwatch": "^0.2.0",
    "ts-md5": "^1.2.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@ckeditor/ckeditor5-angular": "^1.0.0",
    "@ckeditor/ckeditor5-build-classic": "^11.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.21",
    "artillery": "^1.6.0-23",
    "codelyzer": "~4.2.1",
    "dotenv": "^6.0.0",
    "grunt": "^1.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-coveralls": "^2.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "prettier": "1.15.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "~2.7.2"
  }
}
